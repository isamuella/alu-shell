#!/usr/bin/env bash
# display files

if [ -e "school" ]; then
echo "school file exists"
else
if [ ! -e "school" ]; then
echo "school file does not exist"
else
if [ ! -s "school" ]; then
echo "school file is empty"
else
if [ -s "school" ]; then
echo "school file is not empty"
else
if [ -f "school" ]; then
echo "school file is a regular file"
else 
if [ ! -f "school" ]; then
echo ""
