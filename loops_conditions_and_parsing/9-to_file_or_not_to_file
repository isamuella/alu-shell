#!/usr/bin/env bash
# display files

if [ -e "school" ]; then
echo "school file exists"
elif
if [ ! -e "school" ]; then
echo "school file does not exist"
elif
if [ ! -s "school" ]; then
echo "school file is empty"
elif
if [ -s "school" ]; then
echo "school file is not empty"
elif
if [ -f "school" ]; then
echo "school file is a regular file"
elif 
if [ ! -f "school" ]; then
echo ""
